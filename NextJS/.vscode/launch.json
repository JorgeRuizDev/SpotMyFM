{
	"version": "0.2.0",
	"configurations": [
		{
			"type": "pwa-node",
			"request": "launch",
			"name": "Launch Program",
			"skipFiles": [
				"<node_internals>/**"
			],
			"program": "${workspaceFolder}\\src\\styles\\GlobalStyle.tsx",
			"outFiles": [
				"${workspaceFolder}/**/*.js"
			]
		},
		{
			"type": "node-terminal",
			"name": "Run Script: start",
			"request": "launch",
			"command": "npm run start",
			"cwd": "${workspaceFolder}"
		}
	]
}